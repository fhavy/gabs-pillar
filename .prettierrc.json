{
  "singleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "printWidth": 110,
  "arrowParens": "always",
  "importOrder": ["^@ee/(.*)$", "^@lib/(.*)$", "^@components/(.*)$", "^@(server|trpc)/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "endOfLine": "lf"
}
